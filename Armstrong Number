#include <stdio.h>
#include<math.h>

// Code to check Armstrong number - sum of power(no of digits) of digits = number
int main() {
    int lastnumber,number,power=0,sum=0;
    scanf("%d",&number);
    int autonumber = number;
    while(autonumber!=0){
        autonumber = autonumber/10;
        power++ ;
    }
    autonumber = number;
    while(autonumber!=0){
    lastnumber = autonumber%10;
    sum=sum+pow(lastnumber,power);
    autonumber = autonumber/10;
    }
    if(number == sum)
        printf("no is armstrong");
    else
        printf("no is not armstrong");
        return 0 ;
}

